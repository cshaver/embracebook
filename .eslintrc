{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
  ],
  "env": {
    "browser": true,
  },
  "globals": {
    "DEV": true,
  },
  "rules": {
    "no-console": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-shadow": "off",
    "react/no-typos": "off",
    "object-curly-newline": "off",
    "react/forbid-prop-types": "warn",
    "jsx-a11y/anchor-is-valid": [
      "error", {
        "components": [ "Link" ],
        "specialLink": [ "to" ],
      },
    ],
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.config.js",
      },
    },
  },
}
